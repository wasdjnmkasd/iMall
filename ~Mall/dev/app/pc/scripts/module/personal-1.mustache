<div id="personal-1-{{sign}}" v-cloak>
    <div class="personal-1-content">
        <div region="location">
            <div class="location-content">
                <div class="location-name">
                    <b class="fa fa-map-marker fa-fw"></b><span>个人中心</span>
                </div>
            </div>
        </div>
        <div class="personal-content-left">
            <div class="personal-content-left-msg">
                <ul>
                    <li><img src="/images/platform/personal_img01.jpg" alt="中国供销海外购"></li>
                    <li>{{!showName? showName: '游客'}}</li>
                    <li>账户安全：<span>较高</span></li>
                    <li><a href="javascript:void(0)" @click='leftItemAClick' childType="change">修改密码</a></li>
                </ul>
            </div>
            <div class="personal-item-list">

                <div class="personal-content-left-item">
                    <h1>个人设置</h1>
                    <ul>
                        <li @click="leftItemClick"><a href="javascript:void(0)" @click='leftItemAClick' childType="userInfo">个人信息</a></li>
                        <li @click="leftItemClick"><a href="javascript:void(0)" @click='leftItemAClick' childType="address">收货地址</a></li>
                    </ul>
                </div>

                <div class="personal-content-left-item">
                    <h1>订单中心</h1>
                    <ul>
                        <li @click="leftItemClick"><a href="javascript:void(0)" @click='leftItemAClick' childType="order">我的订单</a></li>
                    </ul>
                </div>

                {{#notShow}}
                    <div class="personal-content-left-item">
                        <h1>我的活动</h1>
                        <ul>
                            <li @click="leftItemClick"><a href="javascript:void(0)" @click='leftItemAClick' childType="discount">我的优惠券</a></li>
                        </ul>
                    </div>
                {{/notShow}}

                <div class="personal-content-left-item">
                    <h1>安全中心</h1>
                    <ul>
                        <li @click="leftItemClick"><a href="javascript:void(0)" @click='leftItemAClick' childType="change">修改密码</a></li>
                    </ul>
                </div>

                <div class="personal-content-left-item">
                    <h1>购买指南</h1>
                    <ul>
                        <li @click="leftItemClick"><a href="javascript:void(0)" @click='leftItemAClick' childType="help" childId="11">注册流程</a></li>
                        <li @click="leftItemClick"><a href="javascript:void(0)" @click='leftItemAClick' childType="help" childId="12">购买流程</a></li>
                        <li @click="leftItemClick"><a href="javascript:void(0)" @click='leftItemAClick' childType="help" childId="13">常见问题</a></li>
                        <li @click="leftItemClick"><a href="javascript:void(0)" @click='leftItemAClick' childType="help" childId="14">商品组成</a></li>
                    </ul>
                </div>

                <div class="personal-content-left-item">
                    <h1>支付与配送</h1>
                    <ul>
                        <li @click="leftItemClick"><a href="javascript:void(0)" @click='leftItemAClick' childType="help" childId="21">通关税费</a></li>
                        <li @click="leftItemClick"><a href="javascript:void(0)" @click='leftItemAClick' childType="help" childId="22">配送方式</a></li>
                        <li @click="leftItemClick"><a href="javascript:void(0)" @click='leftItemAClick' childType="help" childId="23">支付方式</a></li>
                        <li @click="leftItemClick"><a href="javascript:void(0)" @click='leftItemAClick' childType="help" childId="24">物流跟踪</a></li>
                    </ul>
                </div>

                <div class="personal-content-left-item">
                    <h1>服务体系</h1>
                    <ul>
                        <li @click="leftItemClick"><a href="javascript:void(0)" @click='leftItemAClick' childType="help" childId="31">会员介绍</a></li>
                        <li @click="leftItemClick"><a href="javascript:void(0)" @click='leftItemAClick' childType="help" childId="32">意见反馈</a></li>
                        <li @click="leftItemClick"><a href="javascript:void(0)" @click='leftItemAClick' childType="help" childId="33">服务承诺</a></li>
                    </ul>
                </div>

                <div class="personal-content-left-item">
                    <h1>关于</h1>
                    <ul>
                        <li @click="leftItemClick"><a href="javascript:void(0)" @click='leftItemAClick' childType="help" childId="41">公司介绍</a></li>
                        <li @click="leftItemClick"><a href="javascript:void(0)" @click='leftItemAClick' childType="help" childId="42">联系我们</a></li>
                        <li><a href="/amount-access.html">商务合作</a></li>
                        <li @click="leftItemClick"><a href="javascript:void(0)" @click='leftItemAClick' childType="help" childId="43">消费者告知书</a></li>
                    </ul>
                </div>
            </div>
        </div>
        <div class="personal-content-right">
            <div region="personalMsg">
                <div class="personal-msg-content" :style="{display:isHide? 'none': 'block'}">
                    <div class="index-content-msg">
                        <img src="/images/platform/member_msg.jpg" class="member_msg" alt="中国供销海外购">
                        <ul>
                            <li orderState="needPay">
                                <dl>
                                    <dt><img src="/images/platform/personal_img02.jpg" alt="中国供销海外购"></dt>
                                    <dd>待付款 <span>{{!needPayCount}}</span></dd>
                                </dl>
                            </li>
                            <li orderState="needDeliver">
                                <dl>
                                    <dt><img src="/images/platform/personal_img02.jpg" alt="中国供销海外购"></dt>
                                    <dd>待发货 <span>{{!needDeliverCount}}</span></dd>
                                </dl>
                            </li>
                            <li orderState="needReceipt">
                                <dl>
                                    <dt><img src="/images/platform/personal_img03.jpg" alt="中国供销海外购"></dt>
                                    <dd>待收货 <span>{{!needReceiptCount}}</span></dd>
                                </dl>
                            </li>
                            <li orderState="finished">
                                <dl>
                                    <dt><img src="/images/platform/personal_img03.jpg" alt="中国供销海外购"></dt>
                                    <dd>已完成 <span>{{!finishedCount}}</span></dd>
                                </dl>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
            <div region="personalBody">
                <div class="personal-order-content" v-if="childType == 'order'">
                    <div class="personal-order-info">
                        <p>
                            <b>我的订单</b>
                            <span class="btn_search" @click="orderSearchEnter"></span>
                            <input  class="contSearch" type="text" placeholder="订单编号" v-model="orderCode" @keyup.enter="orderSearchEnter">
                        </p>
                        <div class="personal-order-list">
                            <div class="personal-order-list-header">
                                <ul>
                                    <li>商品信息</li>
                                    <li>收货人</li>
                                    <li>金额</li>
                                    <li>
                                        <p class="filterState" @click="filterStateShow">
                                            <b>
                                                <span>{{!getFilterText}}</span>
                                                <span class="fa fa-angle-down fa-fw"></span>
                                            </b>
                                        </p>
                                        <ul>
                                            <li status = "0" @click="filterStateSelect">全部</li>
                                            <li status = "1" @click="filterStateSelect">待付款</li>
                                            <li status = "2" @click="filterStateSelect">待发货</li>
                                            <li status = "3" @click="filterStateSelect">待收货</li>
                                            <li status = "4" @click="filterStateSelect">已完成</li>
                                        </ul>
                                    </li>
                                    <li>操作</li>
                                </ul>
                            </div>
                            <div class="personal-order-list-body" v-if="orderList && orderList.length">
                                <div class="personal-order-list-item" :class="getStatusClass(item.status)" v-for="item in orderList">
                                    <h1>
                                        {{!item.createTime.substring(0,item.length-2)}}<span>订单编号：<b>{{!item.orderId}}</b></span>
                                        <span class="createType">{{!getStatusDes(item.createType,item.status)}}</span>
                                    </h1>
                                    <table cellpadding="0" cellspacing="0" :orderId="item.orderId">
                                        <tr v-for="(item2, index2) in item.orderGoodsList">
                                            <td>
                                                <ul>
                                                    <li style="background-size:calc(100% - 16px) calc(100% - 10px);" :style="getLiBackground(item2.itemImg, '/images/platform/default_img.jpg')">
                                                        <div class="img" :style="getImgBackground(item.orderFlag)"></div>
                                                    </li>
                                                    <li>
                                                        <a :href="item2.href">{{!item2.itemName}}</a>
                                                        <div>
                                                            <template v-for="item3 in ergodicInfoList(item2.itemInfo)">
                                                                <span>{{!item3}}</span>
                                                            </template>
                                                        </div>
                                                    </li>
                                                    <li>￥{{!item2.actualPrice.toFixed(2)}}</li>
                                                    <li>x{{!item2.itemQuantity}}</li>
                                                </ul>
                                            </td>
                                            <template v-if="index2 === 0">
                                                <td :rowspan="item.orderGoodsList.length">
                                                    {{!item.orderDetail.receiveName}}
                                                </td>
                                                <td :rowspan="item.orderGoodsList.length">
                                                    <ul>
                                                        <li>总额￥{{!item.orderDetail.payment.toFixed(2)}}</li>
                                                    </ul>
                                                </td>
                                                <td :rowspan="item.orderGoodsList.length">
                                                    <ul>
                                                        <li>{{!getStatusText(item.status,item.orderExpressList,item.orderDetail.payType)}}</li>
                                                        <li><a :href="'/orderDetail.html?orderId=' + item.orderId">订单详情>></a></li>
                                                    </ul>
                                                </td>
                                                <td :rowspan="item.orderGoodsList.length">
                                                    <template v-if="item.status === 6">
                                                        <a href="javascript:void(0)" class="btn btn_confirmDelivery" @click="confirmDeliveryClick(item.orderId)">确认收货</a>
                                                    </template>
                                                    <template v-if="item.status === 0">
                                                        {{#notShow}}
                                                            <a href="javascript:void(0)" class="btn btn_toPay btn_yl" style="width:78px;height:30px;padding:0;" @click="toPayClick('yl', item.orderId, item.orderDetail.payment)">
                                                                <img src="/images/platform/yl.png" style="height:30px;width:72px" alt="中国供销海外购">
                                                            </a>
                                                        {{/notShow}}
                                                        <a href="javascript:void(0)" class="btn btn_toPay btn_wx" style="width:78px;height:30px;padding:0;" @click="toPayClick('wx', item.orderId, item.orderDetail.payment)">
                                                            <img src="/images/platform/wx.png" style="height:30px;width:72px" alt="中国供销海外购">
                                                        </a>
                                                        <a href="javascript:void(0)" class="btn btn_toPay btn_zfb" style="width:78px;height:30px;padding:0;"  @click="toPayClick('zfb', item.orderId, item.orderDetail.payment)">
                                                            <img src="/images/platform/zfb.png" style="height:30px;width:72px" alt="中国供销海外购">
                                                        </a>
                                                    </template>
                                                    <template v-if="item.status !== 0 && item.status !== 7 && item.status !== 8 && item.status !== 9 && item.status !== 21">
                                                        <a href="javascript:void(0)" class="btn btn_refund" @click="refundClick">申请退款</a>
                                                    </template>
                                                    <template v-if="item.status === 7 || item.status === 8 || item.status === 9">
                                                        <a href="javascript:void(0)" class="btn btn_delOrder" @click="delOrderClick(item.orderId)">删除订单</a>
                                                    </template>
                                                    <template v-if="item.status === 0">
                                                        <a href="javascript:void(0)" class="btn btn_closeOrder" @click="closeOrderClick(item.orderId)">关闭订单</a>
                                                    </template>
                                                </td>
                                            </template>
                                        </tr>
                                    </table>
                                </div>
                            </div>
                            <div class="emptyOrder" v-if="!orderList || !orderList.length">
                                <p>您还没有订单，马上去 <a href="/">挑选商品</a> 吧！</p>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="personal-info-content" v-if="childType == 'userInfo'">
                    <div class="personal-info-msg">
                        <p>
                            <b>个人信息</b>
                        </p>
                        <div class="my_msg-list">
                            <ul>
                                <li>尊敬的用户，填写真实的资料，有助于保障您的权益。<span>（手机号码不可再次修改!）</span></li>
                                <li>温馨提示：如果您购买的是跨境保税商品，则您须提交的身份证号码，且与订单支付人、收货人保持一致，以确保订单顺利清关，尽快到达您的身边。</li>
                                <li style="margin-bottom: 0">
                                    {{#hide}}
                                        <span>当前头像：</span>
                                        <div class="img_content">
                                            <img :src="region.response.userDetail.headImg? region.response.userDetail.headImg: '/images/platform/icon_normal_msg.png'" alt="中国供销海外购">
                                            <div class="btn btn_editHeadImg">编辑头像</div>
                                        </div>
                                    {{/hide}}
                                </li>
                                <li>
                                    <span>用户昵称：</span>
                                    <input type="text" class="nickName" id="nickName" name="nickName" :class="userInfoNickNameState === false? 'state_error': ''"
                                           :disabled="request.userInfo.id && !editing" v-model="request.userInfo.userDetail.nickName" @focus="userInfoFocus('nickName')" @blur="userInfoBlur('nickName')">
                                </li>
                                <li>
                                    <span>真实姓名：</span>
                                    <input type="text" class="realname" id="realname" name="realname" :class="userInfoRealNameState === false? 'state_error': ''"
                                           :disabled="request.userInfo.id && !editing" v-model="request.userInfo.userDetail.name" @focus="userInfoFocus('realname')" @blur="userInfoBlur('realname')">
                                </li>
                                <li>
                                    <span>性&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;别：</span>
                                    <ul>
                                        <li>
                                            <input type="radio" id="0" name="sex"
                                                   :disabled="request.userInfo.id && !editing"
                                                   :checked="request.userInfo.userDetail.sex == 0" @change="userInfoChange(0)">
                                            <label for="man">男性</label>
                                        </li>
                                        <li>
                                            <input type="radio" id="1" name="sex"
                                                   :disabled="request.userInfo.id && !editing"
                                                   :checked="request.userInfo.userDetail.sex == 1" @change="userInfoChange(1)">
                                            <label for="women">女性</label>
                                        </li>
                                    </ul>
                                </li>
                                <li>
                                    <span>身份证号：</span>
                                    <input type="text" maxlength="18" class="idNum" id="idNum" name="idNum" :class="userInfoIdNumState === false? 'state_error': ''"
                                           :disabled="request.userInfo.id && !editing" v-model="request.userInfo.userDetail.idNum" @focus="userInfoFocus('idNum')" @blur="userInfoBlur('idNum')">
                                </li>
                                <li>
                                    <span>手机号码：</span>
                                    <input type="text" maxlength="11" class="phone" id="phone" name="phone" readonly="readonly" :class="userInfoPhoneState === false? 'state_error': ''"
                                           :disabled="request.userInfo.id && !editing" v-model="request.userInfo.phone" @focus="userInfoFocus('phone')" @blur="userInfoBlur('phone')">
                                </li>
                                <li>
                                    <span>居住地址：</span>
                                    <input type="text" style="width:350px" class="location" id="location" name="location" :class="userInfoLocationState === false? 'state_error': ''"
                                           :disabled="request.userInfo.id && !editing" v-model="request.userInfo.userDetail.location" @focus="userInfoFocus('location')" @blur="userInfoBlur('location')">
                                </li>
                            </ul>
                            <div class="list-btn">
                                <span class="btn btn_save" v-if="detailExist && editing" @click="userInfoSubmit">保存</span>
                                <span class="btn btn_edit" v-if="detailExist && !editing" @click="userInfoEdit">修改</span>
                                <span class="btn btn_create" v-if="!detailExist" @click="userInfoSubmit">创建</span>
                            </div>
                        </div>
                        <div class="my_msg-changeImg" v-if="selectHeadImg">
                            <p>尊敬的用户，设置您的头像，能完善您的形象哦！</p>
                            <ul class="my_msg-changeImg-btn">
                                <li class="btn_local">
                                    <img src="/images/platform/icon_changeImg01.png" alt="中国供销海外购">
                                    <input type="file" id="headImg" accept="image/gif,image/jpeg,image/jpg,image/png">
                                </li>
                                <li class="btn_photo"><img src="/images/platform/icon_changeImg02.png"></li>
                            </ul>
                            <h1>可支持JPG、PNG、GIF图片文件，且文件小于5M</h1>
                            <img src="/images/platform/icon_normal_msg.png" alt="中国供销海外购">
                            <div class="changeImg-btn">
                                <span class="btn btn_saveImg">保存</span>
                                <span class="btn btn_cancelImg">取消</span>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="personal-address-content" v-if="childType == 'address'">
                    <p class="personal-address-title">
                        <b>收货地址</b>
                    </p>
                    <div class="personal-address-list">
                        <table class="address-table" border="0">
                            <thead>
                            <tr>
                                <th style="width: 10%">收货人</th>
                                <th style="width: 20%">所在地区</th>
                                <th style="width: 30%">详细地址</th>
                                <th style="width: 10%">邮编</th>
                                <th style="width: 10%">手机</th>
                                <th style="width: 20%">操作</th>
                            </tr>
                            </thead>
                            <tbody>
                                <template v-if="addressList.length">
                                    <tr :addressId="item.id" v-for="item in addressList">
                                        <td>{{!item.receiveName}}</td>
                                        <td>{{!item.province}}{{!item.city}}{{!item.area}}</td>
                                        <td>{{!item.address}}</td>
                                        <td>{{!item.zipCode}}</td>
                                        <td>{{!item.receivePhone}}</td>
                                        <td>
                                            <ul>
                                                <li class="btn_edit" @click="addressEditClick">修改</li>
                                                <li class="btn_remove" @click="addressItemDel">删除</li>
                                                <li class="btn btn_default" v-if="item.setDefault"><span>默认地址</span></li>
                                                <li class="btn btn_setDefault" v-else @click="addressDefaultClick"><span>设置默认</span></li>
                                            </ul>
                                        </td>
                                    </tr>
                                </template>
                                <template v-if="!addressList.length">
                                    <tr>
                                        <td></td>
                                        <td></td>
                                        <td></td>
                                        <td></td>
                                        <td></td>
                                        <td></td>
                                    </tr>
                                </template>
                            </tbody>
                            <tfoot>
                                <tr>
                                    <td colspan="6">
                                        您已保存了{{!addressList.length}}个地址</span>
                                    </td>
                                </tr>
                            </tfoot>
                        </table>
                    </div>
                    <div class="personal-address-show">
                        <div class="address-content">
                            <p>新增地址<b class="btn_add" @click="addressAddClick"> + 新增地址</b></p>
                            <div class="address-show-info" :addressId="request.address.id" v-if="request.address.show" style="display: none">
                                <ul>
                                    <li>
                                        <span>所在地区</span>
                                        <div class="picker-country">
                                            <select class="picker-province" :class="province_state === false? 'state_error': ''" :data-name="request.address.province" @change="pickerChange" type="province"></select>
                                            <select class="picker-city"     :class="city_state === false? 'state_error': ''"     :data-name="request.address.city"  @change="pickerChange" type="city"></select>
                                            <select class="picker-area"     :class="area_state === false? 'state_error': ''"     :data-name="request.address.area"  @change="pickerChange" type="area"></select>
                                        </div>
                                    </li>
                                    <li>
                                        <span>详细地址</span>
                                        <textarea cols="30" rows="10" :class="address_state === false? 'state_error': ''" v-model="request.address.address" @focus="addressFocus('address')" @blur="addressBlur('address')"></textarea>
                                    </li>
                                    <li>
                                        <span>邮政编码</span>
                                        <input type="text" maxlength="6" v-model="request.address.zipCode" :class="zipCode_state === false? 'state_error': ''" @focus="addressFocus('zipCode')" @blur="addressBlur('zipCode')">
                                    </li>
                                    <li>
                                        <span>真实姓名</span>
                                        <input type="text" v-model="request.address.receiveName" :class="receiveName_state === false? 'state_error': ''" @focus="addressFocus('receiveName')" @blur="addressBlur('receiveName')">
                                    </li>
                                    <li>
                                        <span>手机号码</span>
                                        <input type="text" maxlength="11" v-model="request.address.receivePhone"  :class="receivePhone_state === false? 'state_error': ''"  @focus="addressFocus('receivePhone')" @blur="addressBlur('receivePhone')">
                                    </li>
                                    <li>
                                        <input type="checkbox" class="checkDefault" v-model="request.address.setDefault">
                                        <i>设置为默认地址</i>
                                    </li>
                                </ul>
                                <div class="address-show-btn">
                                    <span class="btn btn_save" @click="addressSubmit">保存</span>
                                    <span class="btn btn_cancel" @click="addressCancel">取消</span>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="personal-change-content" v-if="childType == 'change'">
                    <div class="personal-change-msg">
                        <p>
                            <b>修改密码</b>
                        </p>
                        <div class="my_msg-list">
                            <ul>
                                <li>尊敬的用户，经常修改密码能增加账户的安全性能。<span>（密码不要设置的过于简单!）</span></li>
                                <li>
                                    <span><b>手机号码</b><span></span></span>
                                    <input type="text" maxlength="11" class="account" v-model="request.change.account" readonly>
                                </li>
                                <li>
                                    <span><b>验证码</b><span></span></span>
                                    <input type="text"  class="validation" :class="changeValidationState === false? 'state_error': ''" v-model="request.change.validation" @focus="changeFocus('validation')" @blur="changeBlur('validation')">
                                    <i>
                                        <span class="doing" v-show="changeTimerVal">发送中<span class="val" style="color:red;padding:0 3px;">{{!changeTimerVal}}</span>s</span>
                                        <span class="control" v-show="!changeTimerVal" @click="getValidationClick">获取验证码</span>
                                    </i>
                                </li>
                                <li>
                                    <span><b>新密码</b><span></span></span>
                                    <input type="password" class="password" :class="changePasswordState === false? 'state_error': ''" v-model="request.change.password" @focus="changeFocus('password')" @blur="changeBlur('password')">
                                </li>
                                <li>
                                    <span><b>确认新密码</b><span></span></span>
                                    <input type="password" class="confirmPassword" :class="changeConfirmPasswordState === false? 'state_error': ''" v-model="request.change.confirmPassword" @focus="changeFocus('confirmPassword')" @blur="changeBlur('confirmPassword')">
                                </li>
                            </ul>
